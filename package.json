{
  "name": "abyss-codex-familiar",
  "displayName": "AbySS Codex Familiar",
  "description": "Custom syntax highlighting for the AbySS programming language.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/liebe-magi/abyss-codex-familiar.git"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "img/logo_256.png",
  "scripts": {
    "compile": "tsc -p .",
    "package": "yarn compile && vsce package",
    "publish": "yarn compile && vsce publish"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "abyss",
        "aliases": [
          "AbySS",
          "abyss"
        ],
        "extensions": [
          ".aby"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "abyss",
        "scopeName": "source.abyss",
        "path": "./syntaxes/abyss.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "abyss",
        "path": "./snippets/abyss.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "typescript": "^5.5.4",
    "vsce": "^2.15.0"
  }
}